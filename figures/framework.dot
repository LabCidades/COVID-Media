digraph graphical_model {
  forcelabels=true;
  graph [overlap=false,
  	 dpi=300,
  	 bgcolor="transparent",
	 esep=8,
         fontsize=12]
  node [shape=oval,
  	width=1.2,
	height=0.6,
	fixedsize=shape,
        fontname=Helvetica]
  Gender    [label="Gender"]
  Age       [label="Age"]
  Fear      [label="Fear"]
  Selfeff   [label="Self-\nEfficacy"]
  Behaviors [label="Protective\nBehaviors"]
  Media     [label="Media\nExposure"]

  {Gender Age} -> {Media Fear}
  {Media Fear Selfeff} -> Behaviors
}
